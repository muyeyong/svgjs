{
  "name": "vue3-demo",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "preview:svg": "svg2js preview -e 'src/assets' -o public -sep /",
    "build:svg": "svg2js build -e 'src/assets' -o public -sep /"
  },
  "dependencies": {
    "@svgjs/cli": "workspace:^",
    "@svgjs/vue-symbol-icon": "workspace:^",
    "tippy.js": "^6.3.7",
    "vue": "^3.3.11",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@tsconfig/node18": "^18.2.2",
    "@types/node": "^18.19.3",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vue/tsconfig": "^0.5.0",
    "npm-run-all2": "^6.1.1",
    "typescript": "~5.3.0",
    "vite": "^5.0.10",
    "vue-tsc": "^1.8.25"
  }
}
